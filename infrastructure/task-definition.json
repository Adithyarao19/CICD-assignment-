{
  "family": "python-web-app",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::<600627318522>:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "python-web-app",
      "image": "<ECR_REPOSITORY_URL>:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ]
}
